/*Complete the function below*/
/*
class Node{
    int data;
    Node next;
    Node(int a){  data = a; next = null; }
}*/

class Solution
{
    //Function to count nodes of a linked list.
    public static int getCount(Node head)
    {
        int len = 0;
        Node curr = head;

        while(curr != null) {
            len++;
            curr = curr.next;
        }

        return len;
    }
}
