

/*
class Node{
    int data;
    Node next;

    Node(int x){
        data = x;
        next = null;
    }
}
*/
class Solution
{

    Node endPtr = null;
    //Function to insert a node at the beginning of the linked list.
    Node insertAtBeginning(Node head, int x) {
        Node temp = new Node(x);
        temp.next = head;
        return temp;
    }

    //Function to insert a node at the end of the linked list.
    Node insertAtEnd(Node head, int x) {
        Node temp = new Node(x);

        if(head == null) {
            endPtr = head;
            return temp;
        }

        if(endPtr == null) {
            Node curr = head;
            while(curr.next != null) {
                curr = curr.next;
            }
            endPtr = curr;
        }

        endPtr.next = temp;
        endPtr = temp;

        return head;
    }
}